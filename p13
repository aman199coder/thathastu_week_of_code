n=int(input("Enter the size of list\n"))
list1=[]
for i in range(0,n):
    element=float(input())
    list1.append(element)
print("The Triplets are:")
for i in range(0,n-2):
    for j in range(i+1,n-1):
        for k in range(j+1,n):
            if((list1[i]+list1[j]+list1[k]>1) and (list1[i]+list1[j]+list1[k]<2)):
                print(i,j,k,end="\n")
