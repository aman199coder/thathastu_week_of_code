n=int(input("Enter the value of n \n"))
a=0
b=1
print("Fibonacci Series:\n")
if(n==1):
    print(a,end="\n")
elif(n==2):
    print(a,end="\n")
    print(b,end="\n")
else:
    print(a,end="\n")
    print(b,end="\n")
    for i in range(1,n-1):
        c=a+b
        print(c,end="\n")
        a=b
        b=c
    
