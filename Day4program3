dic={}
n=int(input("Enter the length of dictionary"))
for i in range(0,n):
    key=input("Enter the value of key")
    value=input("Enter the value of corressponding key")
    dic[key]=value
list1=[]
for i in dic.values():
    list1.append(i)
list1.remove(max(list1))
print("Second Largest value in dictionary:")
print(max(list1))
